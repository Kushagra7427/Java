import java.util.*;

public class Len {
    public static void main(String[] args) {
        StringBuilder sb = new StringBuilder("K");
        sb.append("u");
        sb.append("s");
        sb.append("h");
        sb.append("a");
        sb.append("g");
        sb.append("r");
        sb.append("a");
        System.out.println(sb);
        System.out.println(sb.length());
    }
    
}

