package Looping;

import java.util.Scanner;


public class Table {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the value whose table you want to print");
        int n = sc.nextInt();
        for(int i=1; i<11; i++){
            int t = n * i;
            System.out.println(n+"*"+i+"="+t);
        }

    }
    
}
