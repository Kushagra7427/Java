import java.util.Scanner;

public class Twosum {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a,b;
        int sum;
        System.out.println("Enter Two Numbers");
        a=sc.nextInt();
        b=sc.nextInt();
        sum = a + b;
        System.out.println("Sum of Two Numbers="+sum);

    }
}
